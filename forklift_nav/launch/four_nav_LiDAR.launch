<launch>
  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen" clear_params="true">
    <rosparam file="$(find forklift_nav)/config/nav/LiDAR_four/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find forklift_nav)/config/nav/LiDAR_four/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find forklift_nav)/config/nav/LiDAR_four/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find forklift_nav)/config/nav/LiDAR_four/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find forklift_nav)/config/nav/LiDAR_four/move_base_params.yaml" command="load" />
    <rosparam file="$(find forklift_nav)/config/nav/LiDAR_four/teb_local_planner_params.yaml" command="load" />
  </node>  
</launch>