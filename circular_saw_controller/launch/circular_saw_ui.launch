<launch>
  <node pkg="circular_saw_controller" type="circular_saw" name="circular_saw" output="screen">
    <param name="port" value="/dev/ttyUSB1"/>
    <param name="baudrate" value="115200"/>
    <param name="timeout" value="100"/>
    <param name="log" value="false"/>
  </node>

  <node pkg="circular_saw_controller" type="circular_saw_ui.py" name="circular_saw_ui" output="screen"/>
</launch>
